[alias]
    ad = add
    st = status
    df = diff
    dfc = diff --check
    ct = commit
    cta = commit -a
    ctm = commit -m
    ctam = commit --amend
    logall = log --all
    lg = log --oneline
    lgall = log --oneline --all
    lgraph = log --graph --oneline
    lgraphall = log --graph --all --oneline
    ps = push
    pl = pull
    ft = fetch
    mg = merge
    br = branch
    bra = branch --all
    brall = branch --all
    rt = remote
    co = checkout
    ma = master
    rb = rebase
    rs = restore
    rss = restore --staged
[core]
	excludesfile = /home/vma/.gitignore_global
